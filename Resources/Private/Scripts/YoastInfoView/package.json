{
  "description": "Shel.Neos.YoastSeo:YoastInfoView",
  "license": "GPL-3.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "yarn build-inspector && yarn build-app",
    "watch": "concurrently -k -p \"[{name}]\" -n \"APP,INSPECTOR\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn watch-app\" \"yarn watch-inspector\"",
    "build-inspector": "neos-react-scripts build && uglifyjs --compress --mangle -o ../../../Public/YoastInfoView/Plugin.js -- ../../../Public/YoastInfoView/Plugin.js",
    "watch-inspector": "neos-react-scripts watch",
    "build-app": "webpack -p --progress --colors",
    "build-app-dev": "webpack -d --progress --colors",
    "watch-app": "NODE_ENV=development webpack --watch --progress --colors",
    "update-translations": "bin/download_translations.js -u http://translate.wordpress.org -t wordpress-seo -d '../../Languages'",
    "test-inspector": "testcafe chrome tests/inspector.js"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@neos-project/neos-ui-extensibility": "^3.1.0",
    "@neos-project/react-ui-components": "^3.1.0",
    "babel-loader": "8",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^1.2.0",
    "commander": "^2.19.0",
    "concurrently": "^4.1.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "immer": "^2.1.3",
    "jed": "^1.1.1",
    "lodash.debounce": "^4.0.8",
    "lodash.upperfirst": "^4.3.1",
    "material-ui": "^0.20.2",
    "node-sass": "^4.9.0",
    "po2json": "^1.0.0-alpha",
    "prop-types": "^15.6.0",
    "react": "^16.8.4",
    "react-dom": "^16.6.3",
    "react-intl": "^2.7.2",
    "recompose": "^0.25.1",
    "request": "^2.88.0",
    "sass-loader": "^7.1.0",
    "sassdash": "^0.9.0",
    "style-loader": "^0.19.1",
    "styled-components": "^2.1.2",
    "testcafe": "^1.1.0",
    "testcafe-react-selectors": "^3.1.0",
    "uglifyjs-webpack-plugin": "^2.1",
    "webpack": "^3.12.0",
    "yoast-components": "^4.22.1",
    "yoastseo": "1.49.1"
  },
  "neos": {
    "buildTargetDirectory": "../../../Public/YoastInfoView"
  },
  "browserslist": [
    "last 2 version",
    "> 2%"
  ]
}
